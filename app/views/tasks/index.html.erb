<br />
<div class="row">
    <div class="col-md-2"></div>
    <div class="col-md-10">
        <h1>Your Tasks</h1>
        
        <% @tasks.each do |task| %>
            <li><%= link_to task.name, task_path(task) %></li>
            Status: <%= task.status %>
        <% end %>
    </div>
</div>





